<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!-- Password Generator Variables-->
		<!-- ...pwd... = password-->

		<!-- app settings-->
		<add key="app_name" value="ITAPass"/>

		<!-- generator settings-->
		<add key="max_pwd_length" value="64" />
		<add key="amount_pwd" value="1"/>
		<add key="capital_letters" value="QWERTYUIOPASDFGHJKLZXCVBNM"/>
		<add key="small_letters" value="qwertyuiopasdfghjklzxcvbnm"/>
		<add key="digits" value="0123456789"/>

		<!-- essential settings-->
		<add key="pwd_folder_path" value="\PasswordManager\userdata\passwords\"/>
		<add key="pwd_manager_folder" value="\PasswordManager"/>
		<add key="userdata_folder_path" value="\PasswordManager\userdata\"/>
		<add key="pwd_file_ending" value=".txt"/>
		<add key="pwd_replacement" value="**********"/>
		<add key="master_password_file" value ="master_password"/>
		<add key="master_password_file_ending" value =".yaml"/>
	</appSettings>
</configuration>